{
  "family": "rag-app",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::826695974453:role/rag-app-ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "containerDefinitions": [
    {
      "name": "my-container",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        { "containerPort": 8501, "protocol": "tcp" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/rag-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
